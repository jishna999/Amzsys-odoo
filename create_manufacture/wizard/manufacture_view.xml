<odoo>
    <record id="view_billing_materials_manufacture_form_view" model="ir.ui.view">
        <field name="name">view_billing.materials.manufacture.form.view</field>
        <field name="model">mrp.manufacture</field>
        <field name="arch" type="xml">
            <form string="Create Manufacture">
                <sheet>
                    <group>
                        <field name="mrp_id"/>
                        <field name="mrp_line_ids">
                            <tree editable="bottom">
                                <field name="user_id"/>
                                <field name="product_qty"/>
                                <field name="date_start"/>
                                <field name="date_finished"/>
                            </tree>
                        </field>
                    </group>
                    <button string="Create Manufacture" type="object" name="action_create_manufacture"
                            class="oe_highlight"/>

                </sheet>
            </form>
        </field>
    </record>
</odoo>
